        "attributes": [
          "DOMAIN",
          "TURNS",
          "PARTICIPANTS"
        ],
        "examples": [
          "[TARGET:CONVERSATION:DOMAIN=SUPPORT]",
          "[TARGET:CONVERSATION:TURNS=15:PARTICIPANTS=2]"
        ]
      },
      {
        "token": "SCHEMA",
        "description": "Data schema or structure definition",
        "domains": [
          "DATABASE",
          "API"
        ],
        "attributes": [
          "TYPE",
          "FORMAT",
          "VERSION"
        ],
        "examples": [
          "[TARGET:SCHEMA:TYPE=DATABASE]",
          "[TARGET:SCHEMA:FORMAT=JSON:VERSION=2]"
