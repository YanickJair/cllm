      },
      {
        "token": "TEST",
        "description": "Test case or test suite",
        "domains": [
          "UNIT",
          "INTEGRATION",
          "E2E"
        ],
        "attributes": [
          "TYPE",
          "COVERAGE",
          "STATUS"
        ],
        "examples": [
          "[TARGET:TEST:TYPE=UNIT]",
          "[TARGET:TEST:TYPE=INTEGRATION:COVERAGE=80]"
        ]
      },
      {
        "token": "LOG",
        "description": "System or application log",
        "domains": [
          "SYSTEM",
          "APPLICATION",
          "ERROR"
        ],
        "attributes": [
          "TYPE",
          "LEVEL",
          "SOURCE"
        ],
        "examples": [
          "[TARGET:LOG:TYPE=ERROR]",
          "[TARGET:LOG:LEVEL=DEBUG:SOURCE=API]"
        ]
      },
      {
        "token": "RECORD",
        "description": "Database record or entry",
        "domains": [
          "DATABASE",
          "USER",
          "TRANSACTION"
        ],
        "attributes": [
          "TYPE",
          "STATUS",
          "ID"
        ],
        "examples": [
          "[TARGET:RECORD:TYPE=USER]",
          "[TARGET:RECORD:TYPE=TRANSACTION:STATUS=PENDING]"
        ]
      }
    ]
  },
