{
  "REQ": {
    "name": "Request/Action Tokens",
    "description": "Primary action the user wants performed",
    "color": "blue",
    "format": "[REQ:ACTION:MODIFIER]",
    "totalTokens": 15,
    "tokens": [
      {
        "token": "ANALYZE",
        "synonyms": [
          "analyze",
          "review",
          "examine",
          "evaluate",
          "assess",
          "inspect"
        ],
        "description": "Deep analysis or examination of content",
        "examples": [
          "Analyze this code for bugs",
          "Review this document carefully",
          "Examine the data patterns"
        ],
        "modifiers": [
          "DEEP",
          "SURFACE",
          "QUICK"
        ],
        "usage": "[REQ:ANALYZE] or [REQ:ANALYZE:DEEP]"
      },
      {
        "token": "EXTRACT",
        "synonyms": [
          "extract",
          "pull out",
          "identify",
          "find",
          "locate",
          "get"
        ],
        "description": "Pull out specific information from content",
        "examples": [
          "Extract all dates from the text",
          "Find customer names",
          "Identify key points"
        ],
        "modifiers": [
          "ALL",
          "FIRST",
          "UNIQUE"
        ],
        "usage": "[REQ:EXTRACT] or [REQ:EXTRACT:ALL]"
      },
      {
        "token": "GENERATE",
        "synonyms": [
          "generate",
          "create",
          "write",
          "draft",
          "compose",
          "produce"
        ],
        "description": "Create new content",
        "examples": [
          "Generate a report",
          "Write an email",
          "Create test cases"
        ],
        "modifiers": [
          "CREATIVE",
          "FACTUAL"
        ],
        "usage": "[REQ:GENERATE:TYPE=EMAIL]"
      },
      {
        "token": "SUMMARIZE",
        "synonyms": [
          "summarize",
          "condense",
          "brief",
          "synopsis"
        ],
        "description": "Create shorter version of content",
        "examples": [
          "Summarize this article",
          "Give me the key points",
          "Brief summary needed"
        ],
        "modifiers": [
          "BRIEF",
          "DETAILED"
        ],
        "usage": "[REQ:SUMMARIZE:BRIEF]"
      },
      {
        "token": "TRANSFORM",
        "synonyms": [
          "convert",
          "transform",
          "change",
          "rewrite",
          "translate"
        ],
        "description": "Convert from one format/style to another",
        "examples": [
          "Convert to JSON",
          "Translate to Spanish",
          "Rewrite in formal tone"
        ],
        "modifiers": [
          "FORMAT",
          "STYLE",
          "LANGUAGE"
        ],
        "usage": "[REQ:TRANSFORM:TO=JSON]"
      },
      {
        "token": "EXPLAIN",
        "synonyms": [
          "explain",
          "describe",
          "clarify",
          "elaborate"
        ],
        "description": "Provide explanation or description",
        "examples": [
          "Explain how this works",
          "Describe the process",
          "Clarify the concept"
        ],
        "modifiers": [
          "SIMPLE",
          "DETAILED",
          "TECHNICAL"
        ],
        "usage": "[REQ:EXPLAIN:SIMPLE]"
      },
      {
        "token": "COMPARE",
        "synonyms": [
          "compare",
          "contrast",
          "versus",
          "difference"
        ],
        "description": "Compare multiple items or concepts",
        "examples": [
          "Compare React vs Vue",
          "Show differences between X and Y",
          "Contrast these approaches"
        ],
        "modifiers": null,
        "usage": "[REQ:COMPARE]"
      },
      {
        "token": "CLASSIFY",
        "synonyms": [
          "classify",
          "categorize",
          "sort",
          "group",
          "label"
        ],
        "description": "Put content into categories",
        "examples": [
          "Classify these tickets by type",
          "Categorize customer feedback",
          "Sort by priority"
        ],
        "modifiers": [
          "BY_TYPE",
          "BY_PRIORITY"
        ],
        "usage": "[REQ:CLASSIFY:BY=TYPE]"
      },
      {
        "token": "DEBUG",
        "synonyms": [
          "debug",
          "fix",
          "troubleshoot",
          "diagnose"
        ],
        "description": "Identify and fix issues",
        "examples": [
          "Debug this code",
          "Find what's wrong",
          "Fix the error"
        ],
        "modifiers": null,
        "usage": "[REQ:DEBUG]"
      },
      {
        "token": "OPTIMIZE",
        "synonyms": [
          "optimize",
          "improve",
          "enhance",
          "refactor"
        ],
        "description": "Make content better or more efficient",
        "examples": [
          "Optimize this query",
          "Improve performance",
          "Refactor the code"
        ],
        "modifiers": [
          "PERFORMANCE",
          "READABILITY"
        ],
        "usage": "[REQ:OPTIMIZE:PERFORMANCE]"
      },
      {
        "token": "VALIDATE",
        "synonyms": [
          "validate",
          "verify",
          "check",
          "confirm"
        ],
        "description": "Check correctness or validity",
        "examples": [
          "Validate this data",
          "Check if correct",
          "Verify the format"
        ],
        "modifiers": null,
        "usage": "[REQ:VALIDATE]"
      },
      {
        "token": "SEARCH",
        "synonyms": [
          "search",
          "query",
          "lookup",
          "find"
        ],
        "description": "Search for specific information",
        "examples": [
          "Search for mentions of X",
          "Find all instances",
          "Look up the definition"
        ],
        "modifiers": [
          "ALL",
          "FIRST"
        ],
        "usage": "[REQ:SEARCH:ALL]"
      },
      {
        "token": "RANK",
        "synonyms": [
          "rank",
          "prioritize",
          "order",
          "sort"
        ],
        "description": "Order items by importance or value",
        "examples": [
          "Rank by priority",
          "Sort by importance",
          "Order by relevance"
        ],
        "modifiers": null,
        "usage": "[REQ:RANK:BY=PRIORITY]"
      },
      {
        "token": "PREDICT",
        "synonyms": [
          "predict",
          "forecast",
          "estimate",
          "project"
        ],
        "description": "Make predictions based on data",
        "examples": [
          "Predict next quarter sales",
          "Forecast trends",
          "Estimate completion time"
        ],
        "modifiers": null,
        "usage": "[REQ:PREDICT]"
      },
      {
        "token": "FORMAT",
        "synonyms": [
          "format",
          "structure",
          "organize"
        ],
        "description": "Apply specific formatting or structure",
        "examples": [
          "Format as table",
          "Structure this data",
          "Organize by sections"
        ],
        "modifiers": null,
        "usage": "[REQ:FORMAT:AS=TABLE]"
      }
    ]
  },
  "TARGET": {
    "name": "Target Object Tokens",
    "description": "What the action is applied to",
    "color": "purple",
    "format": "[TARGET:TYPE:DOMAIN:ATTRIBUTES]",
    "totalTokens": 20,
    "tokens": [
      {
        "token": "CODE",
        "description": "Source code or script",
        "domains": [
          "GENERAL",
          "SPECIFIC_LANGUAGE"
        ],
        "attributes": [
          "LANG",
          "FILE_TYPE",
          "SIZE"
        ],
        "examples": [
          "[TARGET:CODE:LANG=PYTHON]",
          "[TARGET:CODE:LANG=JAVASCRIPT:FILE_TYPE=MODULE]"
        ]
      },
      {
        "token": "TRANSCRIPT",
        "description": "Conversation or dialogue transcript",
        "domains": [
          "SUPPORT",
          "MEETING",
          "INTERVIEW",
          "CALL"
        ],
        "attributes": [
          "DOMAIN",
          "TYPE",
          "DURATION",
          "PARTICIPANTS"
        ],
        "examples": [
          "[TARGET:TRANSCRIPT:DOMAIN=SUPPORT]",
          "[TARGET:TRANSCRIPT:TYPE=MEETING:PARTICIPANTS=5]"
        ]
      },
      {
        "token": "EMAIL",
        "description": "Email message",
        "domains": [
          "BUSINESS",
          "SUPPORT",
          "MARKETING"
        ],
        "attributes": [
          "DOMAIN",
          "PRIORITY",
          "DIRECTION"
        ],
        "examples": [
          "[TARGET:EMAIL:DOMAIN=BUSINESS]",
          "[TARGET:EMAIL:PRIORITY=HIGH:DIRECTION=OUTBOUND]"
        ]
      },
      {
        "token": "DOCUMENT",
        "description": "General document or text",
        "domains": [
          "GENERAL",
          "LEGAL",
          "TECHNICAL",
          "BUSINESS"
        ],
        "attributes": [
          "TYPE",
          "DOMAIN",
          "LENGTH",
          "FORMAT"
        ],
        "examples": [
          "[TARGET:DOCUMENT:TYPE=LEGAL]",
          "[TARGET:DOCUMENT:DOMAIN=TECHNICAL:FORMAT=PDF]"
        ]
      },
      {
        "token": "DATA",
        "description": "Structured or unstructured data",
        "domains": [
          "GENERAL"
        ],
        "attributes": [
          "FORMAT",
          "SIZE",
          "SOURCE",
          "ROWS",
          "COLS"
        ],
        "examples": [
          "[TARGET:DATA:FORMAT=CSV:ROWS=1000]",
          "[TARGET:DATA:FORMAT=JSON:SIZE=LARGE]"
        ]
      },
      {
        "token": "TICKET",
        "description": "Support or service ticket",
        "domains": [
          "SUPPORT",
          "IT",
          "MAINTENANCE"
        ],
        "attributes": [
          "DOMAIN",
          "STATUS",
          "PRIORITY",
          "COUNT"
        ],
        "examples": [
          "[TARGET:TICKET:DOMAIN=SUPPORT:PRIORITY=HIGH]",
          "[TARGET:TICKET:COUNT=50]"
        ]
      },
      {
        "token": "COMPLAINT",
        "description": "Customer complaint",
        "domains": [
          "SUPPORT"
        ],
        "attributes": [
          "ISSUE",
          "SEVERITY",
          "CHANNEL"
        ],
        "examples": [
          "[TARGET:COMPLAINT:ISSUE=SHIPPING_DELAY]",
          "[TARGET:COMPLAINT:SEVERITY=HIGH:CHANNEL=EMAIL]"
        ]
      },
      {
        "token": "QUERY",
        "description": "Database query or search query",
        "domains": [
          "DATABASE",
          "SEARCH"
        ],
        "attributes": [
          "LANG",
          "DATABASE",
          "COMPLEXITY"
